.col-sm-4.col-md-3.proto-content
  .thumbnail
    = link_to image_tag(proto.images.main.first.photo), prototype_path(proto)
    .caption
      %h3= proto.title
      .proto-meta
        .proto-user
          = proto.user.email
        .proto-posted
      %ul.proto-tag-list.list-inline
        %li
          %a{href: "#", class: "btn btn-default"} iPad
        %li
          %a{href: "#", class: "btn btn-default"} wireframe
